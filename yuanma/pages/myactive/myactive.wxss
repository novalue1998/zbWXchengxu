page{background: #f5f5f5;}
.top{display: flex;margin-top: 20rpx;height: 80rpx;background: #ffffff;align-items: center;}
.order_sn{margin-left: 30rpx;display: flex;}
.status{display: flex;right: 40rpx;position: absolute;color: #999999;}
.middle{background: #f9f9f9;display: flex;}
.left image{height: 140rpx;width: 210rpx;margin-left: 30rpx;margin-top: 15rpx;margin-bottom: 10rpx;}
.right{margin-top: 20rpx;margin-left:30rpx;width: 60%;font-size: 28rpx;}
.rightMiddle{font-size: 24rpx;display: flex;margin-top: 45rpx;}
.address{color: #999999;}
.consume_jf{color: #f23030;display: flex;right: 40rpx;position: absolute;}
.status1{display: flex;right: 40rpx;position: absolute;color: #0099ff;}
.foot{display: flex;height: 100rpx;background: #ffffff;}
.call{height: 18rpx;width: 18rpx;}
.time{margin-top: 38rpx;font-size: 24rpx;color: #999999;margin-left: 30rpx; overflow: hidden; text-overflow: ellipsis;white-space: nowrap;}
.callshop{font-size: 26rpx;color: #666666;border-radius: 27rpx;padding: 10rpx 14rpx;background: #ffffff;display: flex;margin-top: 22rpx;margin-bottom: 22rpx;align-items: center;right: 30rpx;position: absolute;border: 1rpx solid #999999;}
.callmkt{font-size: 26rpx;color: #666666;border: 1rpx solid #999999;border-radius: 27rpx;padding: 16rpx 18rpx 16rpx 18rpx;background: #ffffff;display: flex;margin-top: 22rpx;margin-bottom: 22rpx;align-items: center;margin-left: 20rpx}
.showTicket{font-size: 26rpx;color: #ffffff;border-radius: 27rpx;padding: 10rpx 14rpx;background: #f23030;display: flex;margin-top: 22rpx;margin-bottom: 22rpx;align-items: center;right: 30rpx;position: absolute;margin-left: 10rpx;}
.mkt{margin-left: 8rpx;}
.myactive:last-child {margin-bottom: 100rpx;}
.all{
    text-align: center;
    font-size: 30rpx;
    padding: 0 0 40rpx;
    margin-top: 25rpx;
}
.mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 9999;
    opacity: 0.7;
}

.modalDlg {
    height: 880rpx;
    width: 85%;
    position: fixed;
    top: 60rpx;
    left: 0;
    right: 0;
    z-index: 9999;
    margin: 0 auto;
    background-color: #fff;
    border-radius: 20rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.off image{
  width: 80rpx;
    height: 80rpx;
   position: absolute;
   top: 900rpx;
   left: 50%;
   margin-left: -40rpx;
}
