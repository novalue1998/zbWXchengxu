}

.on {
    width: 94%;
    overflow-x: hidden;
    height: 375rpx;
    position: relative;
    margin-top: 10rpx;
    border: 1px solid rgb(250, 250, 250);
    margin-right: 5%;
    z-index: -1;
}

.open {
    font-size: 25px;
    display: block;
    padding-top: 15%;
