}

.page__bd {
    margin-top: 20px;
    width: 100%;
}

.slide-image {
    width: 90%;
    margin-left: 5%;
    height: 160px;
}

.weui-flex {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    margin-top: 20px;
    width: 100%;
    overflow: hidden;
    /* position: relative; */
}
.shopCoupon{   
    display: flex;
    flex-direction: row;
